    "jquery": "^3.1.0",
    "lodash": "^4.16.2",
    "vue": "^2.0.1",
    "vue-i18n": "^4.8.0",
    "vue-router": "^2.1.1",
    "vue-resource": "^1.0.3"
    "vuex": "^2.1.1"
  }
